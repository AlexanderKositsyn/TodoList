{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "alias": {
          "containers": "./src/containers",
          "components": "./src/components"
        }
      }
    ]
  ]
}
